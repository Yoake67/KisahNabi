<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView_kisaharasul"
        android:layout_width="117dp"
        android:layout_height="118dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_kisahrasul" />

    <ImageView
        android:id="@+id/imageView2_quiz"
        android:layout_width="117dp"
        android:layout_height="118dp"
        app:layout_constraintBottom_toBottomOf="@+id/my_image_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/my_image_view"
        app:srcCompat="@drawable/ic_quiz" />

    <ImageView
        android:id="@+id/my_image_view"
        android:layout_width="117dp"
        android:layout_height="118dp"
        android:src="@drawable/ic_kisahnabi"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_kisaharasul"
        app:layout_constraintEnd_toStartOf="@+id/imageView2_quiz"
        app:layout_constraintStart_toEndOf="@+id/imageView_kisaharasul"
        app:layout_constraintTop_toTopOf="@+id/imageView_kisaharasul" />

    <TextView
        android:id="@+id/teks_atas"
        android:layout_width="269dp"
        android:layout_height="45dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/adlam_display"
        android:paddingTop="12dp"
        android:text="KISAH 25 NABI DAN RASUL"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textDirection="firstStrong"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>